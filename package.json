{
  "name": "curb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "mobile-start:development": "eval \"$(paste -s -d ' ' .env.development) react-native start --reset-cache\"",
    "mobile-build-android:development": "eval \"$(paste -s -d ' ' .env.development) react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/\"",
    "mobile-build-ios:development": "eval \"$(paste -s -d ' ' .env.development) react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/curb/main.jsbundle --assets-dest ios/curb/assets/\"",
    "mobile-build-android:production": "eval \"$(paste -s -d ' ' .env.production) react-native bundle --platform android --dev false --entry-file index.production.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/\"",
    "mobile-build-ios:production": "eval \"$(paste -s -d ' ' .env.production) react-native bundle --platform ios --dev false --entry-file index.production.js --bundle-output ios/curb/main.jsbundle --assets-dest ios/curb/assets/\"",
    "mobile-run-android:development": "eval \"$(paste -s -d ' ' .env.development) react-native run-android\"",
    "mobile-run-android:production": "eval \"$(paste -s -d ' ' .env.production) react-native run-android\"",
    "mobile-run-ios:development": "eval \"$(paste -s -d ' ' .env.development) react-native run-ios\"",
    "mobile-run-ios:production": "eval \"$(paste -s -d ' ' .env.production) react-native run-ios\"",
    "mobile-eject": "react-native eject",
    "web-start:development": "eval \"$(paste -s -d ' ' .env.development) react-scripts start\"",
    "web-start:production": "eval \"$(paste -s -d ' ' .env.production) react-scripts start\"",
    "web-build:development": "eval \"$(paste -s -d ' ' .env.development) react-scripts build\"",
    "web-build:production": "eval \"$(paste -s -d ' ' .env.production) react-scripts build\"",
    "web-eject": "react-scripts eject"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "i18next": "^11.6.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-device-detect": "^1.6.2",
    "react-dom": "^16.4.2",
    "react-i18next": "^9.0.2",
    "react-native": "^0.57.5",
    "react-native-camera": "^1.13.1",
    "react-native-image-picker": "^0.28.0",
    "react-native-vector-icons": "^6.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-native": "^4.3.0",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "metro-react-native-babel-preset": "^0.49.2",
    "prettier": "^1.14.2"
  }
}
